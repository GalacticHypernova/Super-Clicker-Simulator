getgenv().settings={
   ["click"]=false,
   ["upgrade"]=false,
   ["egg"]=false,
   ["collect"]=false,
   ["rebirth"]=false,
   ["skins"]=false,
   ["super"]=false
}
function codes()
    task.spawn(function()
        local args={
        }
        game:GetService("ReplicatedStorage").Remotes.RedeemCode:InvokeServer(unpack(args))
    end)
end
function buyEgg(eggType)
    task.spawn(function()
        while getgenv().settings["egg"] and task.wait() do
            local args = {
                [1] = workspace.PromptEgg[eggType],
                [2] = "E"
            }
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\240\159\165\154"):InvokeServer(unpack(args))
        end
    end)
end
function rebirth(rebirthAmount)
    task.spawn(function()
        while getgenv().settings["rebirth"] and task.wait() do
           game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\239\184\189"):FireServer(rebirthAmount)
        end
    end)
end
function autoUp()
    task.spawn(function()
        while getgenv().settings["upgrade"] and task.wait() do
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\239\184\189UPG"):FireServer("TapsUpgrade")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\239\184\189UPG"):FireServer("SlotsUpgrade")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\239\184\189UPG"):FireServer("PetsUpgrade")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\239\184\189UPG"):FireServer("SpeedUpgrade")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\239\184\189UPG"):FireServer("EvolvePetsUpgrade")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\239\184\189UPG"):FireServer("AutoTapUpgrade")
        end
    end)
end
function autoCollect()
    task.spawn(function()
        while getgenv().settings["collect"] and task.wait() do
            for i,v in pairs(game:GetService("Workspace").Coletaveis:GetDescendants()) do
                if v.Name=="TouchInterest" and v.Parent.ClassName=="MeshPart" then
                    local plyr=game.Players.LocalPlayer.Character.HumanoidRootPart
                    firetouchinterest(plyr, v.Parent ,0)
                    wait()
                    firetouchinterest(plyr,v.Parent,1)
                end
            end
        end
    end)
end
local RankArray={}
for i=2,12,1 do
    table.insert(RankArray,i)
end
function super(superAmount)
    task.spawn(function()
        while getgenv().settings["super"] and task.wait() do
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161SUPER"):FireServer(superAmount)
        end
    end)
end
function click()
   task.spawn(function()
       while getgenv().settings["click"] and task.wait()do
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161\240\159\150\177\239\184\143"):FireServer()
       end
   end)
end
function rank()
    task.spawn(function()
        while getgenv().settings["rank"] and task.wait() do
            for rankx=1,#RankArray do
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161BR"):InvokeServer(tostring(RankArray[rankx]))
            end
        end
    end)
end
function teleportTO(placeCFrame) 
    local player = game.Players.LocalPlayer 
    if player.Character then 
        player.Character.HumanoidRootPart.CFrame=placeCFrame 
    end 
end
function teleportWorld(World) 
    if game:GetService("Workspace").Portals:FindFirstChild(World) then 
        teleportTO(game:GetService("Workspace").Portals[World].TP.CFrame) 
    end 
end
function skins()
    task.spawn(function()
        while getgenv().settings["skins"] and task.wait() do
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161BS"):InvokeServer("1")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161BS"):InvokeServer("2")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161BS"):InvokeServer("3")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161BS"):InvokeServer("4")
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("\226\154\161BS"):InvokeServer("5")
        end
    end)
end
local library = loadstring(game:GetObjects("rbxassetid://7657867786")[1].Source)()
local Wait = library.subs.Wait -- Only returns if the GUI has not been terminated. For 'while Wait() do' loops

local PepsisWorld = library:CreateWindow({
Name = "Pepsi's World",
Themeable = {
Info = "Discord Server: VzYTJ7Y"
}
})

local GeneralTab = PepsisWorld:CreateTab({
Name = "General"
})
local FarmingSection = GeneralTab:CreateSection({
Name = "Autofarm"
})
local PetSection = GeneralTab:CreateSection({
Name = "Pets"
})
local Other = GeneralTab:CreateSection({
  Name = "Misc",
  Side="Right"
})
FarmingSection:AddToggle({
Name = "Auto Click",
Callback=function(bool) getgenv().settings["click"]=bool
  if bool then
      click()
  end
end,
Keybind =  {
Mode="Dynamic"
}
})
FarmingSection:AddToggle({
    Name="Auto Upgrade everything",
    Callback=function(bool)
        getgenv().settings["upgrade"]=bool
        if bool then
            autoUp()
        end
    end
    
})
local selectedRebirth
FarmingSection:AddTextbox({
    Name="Auto rebirth amount (1-15)",
    Callback=function(value)
        selectedRebirth=value
        print(value)
    end
})
FarmingSection:AddToggle({
    Name="Auto rebirth",
    Callback=function(bool)
        getgenv().settings["rebirth"] =bool
        if bool and selectedRebirth then
            rebirth(tostring(selectedRebirth))
        end
    end
})
FarmingSection:AddToggle({
    Name="Auto rank",
    Callback=function(bool)
        getgenv().settings["rank"]=bool
        if bool then
            rank()
        end
    end
})
local selectedSuper
FarmingSection:AddTextbox({
    Name="Super rebirth amount(5M each)",
    Callback=function(value)
        selectedSuper=value
        print(value)
    end
})
FarmingSection:AddToggle({
    Name="Auto super rebirth",
    Callback=function(bool)
        getgenv().settings["super"] =bool
        if bool and selectedSuper then
            super(tonumber(selectedSuper))
        end
    end
})
FarmingSection:AddToggle({
    Name="Auto buy skins",
    Callback=function(bool)
        getgenv().settings["skins"]=bool
        if bool then
            skins()
        end
    end
})
FarmingSection:AddToggle({
    Name="Auto collect diamonds",
    Callback=function(bool)
        getgenv().settings["collect"]=bool
        if bool then
            autoCollect()
        end
    end
})
local selectedEgg
PetSection:AddDropdown({
   Name="Egg to buy",
   List={"Click Me","Common","Uncommon","Rare","Epic","Legendary","Angelical","Hacker","Russo"},
   Callback=function(value)
       selectedEgg=value
       print(value)
       end
})
PetSection:AddToggle({
   Name="Buy egg",
   Callback=function(bool)
       getgenv().settings["egg"]=bool
       if bool and selectedEgg then
       buyEgg(tostring(selectedEgg))
       end
   end
})
local selectedWorld
Other:AddDropdown({
    Name="Teleport to world",
    List={"Click Me","Crystal Island","Vulcan Island","Forest Island","Angel Island","Mythical Island","Hacker Island","Russo Island","Training Island"},
    Callback=function(value)
        selectedWorld=value
        print(value)
    end
})
Other:AddButton({
    Name="Teleport",
    Callback=function()
        if selectedWorld then
        teleportWorld(selectedWorld)
        end
    end
})
Other:AddSlider({
    Name="Walkspeed",
    Value=16,
    Min=16,
    Max=1000,
    Callback=function(value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=value
    end
})
Other:AddSlider({
    Name="Jump power",
    Value=50,
    Min=50,
    Max=1000,
    Callback=function(value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower=value
    end
})
